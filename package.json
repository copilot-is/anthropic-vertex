{
  "name": "anthropic-vertex-ai-provider",
  "version": "0.1.4",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"src/**/*.ts*\"",
    "prettier-write": "prettier --write \"src/**/*.ts*\"",
    "test": "pnpm test:node && pnpm test:edge",
    "test:ci": "pnpm test:node && pnpm test:edge",
    "test:edge": "vitest --config vitest.edge.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/provider": "^0.0.22",
    "@ai-sdk/provider-utils": "^1.0.17",
    "google-auth-library": "^9.12.0"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^4.0.2",
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.1",
    "eslint": "^9.9.1",
    "prettier": "3.3.3",
    "tsup": "^8.2.4",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.3.0",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://sdk.vercel.ai/providers/community-providers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fry69/anthropic-vertex-ai-provider.git"
  },
  "bugs": {
    "url": "https://github.com/fry69/anthropic-vertex-ai-provider/issues"
  },
  "keywords": [
    "ai",
    "vertexai",
    "anthropic",
    "claude"
  ],
  "packageManager": "pnpm@8.6.9",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "printWidth": 100
  }
}
